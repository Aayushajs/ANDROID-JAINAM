{
  "expo": {
    "name": "E-Pharmacy",
    "slug": "E-Pharmacy",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.2.0",
    "updates": {
      "url": "https://u.expo.dev/9096ee65-922d-487f-9717-475dc87fe571",
      "fallbackToCacheTimeout": 0
    },
    "newArchEnabled": true,
    "orientation": "portrait",
    "description": "E-Pharmacy is a modern mobile application that allows users to order medicines online, upload prescriptions, track orders, and access verified healthcare resources in one place.",
    "icon": "./assets/icon1.png",
    "scheme": "epharmacy",
    "userInterfaceStyle": "automatic",
    "primaryColor": "#FFFFFF",
    "backgroundColor": "#FFFFFF",
    "splash": {
      "image": "./assets/icon1.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.epharmacy.app",
      "splash": {
        "image": "./assets/icon1.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires access to the camera for scanning prescriptions and uploading documents.",
        "NSPhotoLibraryUsageDescription": "This app requires access to photos to upload prescriptions."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon1.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "image": "./assets/icon1.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "package": "com.epharmacy.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/icon1.png",
      "themeColor": "#FFFFFF",
      "backgroundColor": "#FFFFFF"
    },
    "extra": {
      "eas": {
        "projectId": "9096ee65-922d-487f-9717-475dc87fe571"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
